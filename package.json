{
  "name": "cph-leetcode-ash",
  "displayName": "CPH Leetcode Ash",
  "description": "CPH for Leetcode! Supports multiple languages",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.96.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onCommand:cph-leetcode-ash.fetch",
    "onCommand:cph-leetcode-ash.run"
  ],
  "main": "./src/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "cph-leetcode-ash.fetch",
        "title": "Fetch Leetcode Test Cases"
      },
      {
        "command": "cph-leetcode-ash.run",
        "title": "Run Leetcode Test Cases"
      }
    ]
  },
  "scripts": {
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "vscode-test"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.10",
    "@types/node": "20.x",
    "@types/vscode": "^1.96.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "eslint": "^9.16.0"
  },
  "dependencies": {
    "jsdom": "^26.0.0"
  }
}
